{
  "name": "midi-player-js",
  "version": "1.1.1",
  "description": "Midi parser & player engine for browser or Node. Works well with single or multitrack MIDI files.",
  "main": "module/index.js",
  "scripts": {
    "pretest": "npm run build",
    "test": "mocha",
    "build": "mkdir -p module browser && babel src --out-file ./module/index.js && npm run browser && ls -l ./module/",
    "browser": "browserify -r ./module/index.js:MidiPlayer > ./browser/index.js && uglifyjs ./browser/index.js -m > ./browser/index.min.js",
    "docs": "./node_modules/.bin/jsdoc -d ./docs src -t ./node_modules/minami"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "browserify": "^14.1.0",
    "jsdoc": "^3.4.3",
    "minami": "^1.1.1",
    "mocha": "~2.4.5",
    "uglify-js": "^2.7.3"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/grimmdude/MidiPlayerJS.git"
  },
  "bugs": {
    "url": "https://github.com/grimmdude/MidiPlayerJS/issues"
  },
  "keywords": [
    "midi",
    "parser",
    "player"
  ],
  "author": "Garrett Grimm",
  "license": "MIT",
  "babel": {
    "presets": [
      "es2015"
    ],
    "sourceMaps": "inline"
  }
}
